{
  "name": "@bowenqt/qiniu-ai-sdk",
  "version": "0.30.3",
  "description": "TypeScript SDK for Qiniu Cloud AI Token API - Chat, Image, Video generation and more",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./node": {
      "require": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.js"
      },
      "import": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.mjs"
      }
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.mjs"
    },
    "./adapter": {
      "require": {
        "types": "./dist/adapter/index.d.ts",
        "default": "./dist/adapter/index.js"
      },
      "import": {
        "types": "./dist/adapter/index.d.ts",
        "default": "./dist/adapter/index.mjs"
      }
    },
    "./ai-tools": {
      "require": {
        "types": "./dist/ai-tools/index.d.ts",
        "default": "./dist/ai-tools/index.js"
      },
      "import": {
        "types": "./dist/ai-tools/index.d.ts",
        "default": "./dist/ai-tools/index.mjs"
      }
    }
  },
  "bin": {
    "qiniu-mcp-server": "./bin/qiniu-mcp-server.mjs"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "qiniu",
    "ai",
    "llm",
    "openai",
    "chat",
    "image-generation",
    "video-generation",
    "sdk",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bowenQT/qiniu-ai-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/bowenQT/qiniu-ai-sdk/issues"
  },
  "homepage": "https://github.com/bowenQT/qiniu-ai-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/provider": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@ai-sdk/provider": "^2.0.0-canary.14",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.16",
    "ai": "^5.0.0-canary.24",
    "typescript": "^5.0.0",
    "vitest": "^4.0.16",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.25.2"
  }
}